<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane prefHeight="417.0" prefWidth="293.0">
         <children>
            <Button fx:id="pwd" layoutX="21.0" layoutY="367.0" mnemonicParsing="false" onAction="#presentDirectory" text="pwd" />
            <Button fx:id="cd" layoutX="68.0" layoutY="367.0" mnemonicParsing="false" onAction="#currentDirectory" text="cd" />
            <Button fx:id="cdf" layoutX="109.0" layoutY="367.0" mnemonicParsing="false" onAction="#currentInFolder" text="cd in folder" />
            <Button fx:id="mkdir" layoutX="192.0" layoutY="367.0" mnemonicParsing="false" onAction="#makeDir" text="mkdir" />
            <Button fx:id="ls" layoutX="249.0" layoutY="367.0" mnemonicParsing="false" onAction="#list" text="ls" />
            <TextField fx:id="text" layoutX="73.0" layoutY="334.0" />
            <Label layoutX="77.0" layoutY="14.0" text="Easy Command Line">
               <font>
                  <Font name="System Bold" size="15.0" />
               </font>
            </Label>
            <Label layoutX="102.0" layoutY="310.0" text="Command String" />
            <Label fx:id="out" layoutX="7.0" layoutY="39.0" prefHeight="281.0" prefWidth="281.0" text="Output here:" wrapText="true" />
         </children></AnchorPane>
   </children>
</GridPane>
